		return;
	}
	
		
		
if(viewby=='week'){	  
  var data = google.visualization.arrayToDataTable(<?php echo $gchartdata_week; ?>);
}else if(viewby=='month'){
   var data = google.visualization.arrayToDataTable(<?php echo $gchartdata_month; ?>);
}else if(viewby=='day'){
