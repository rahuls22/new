    function GetAVG($resAry,$type){
	  if($resAry){
	  $count=0;
	  $totRct = count($resAry);
	  for($i=0;$i<$totRct;$i++){
	   $count = $count + $resAry[$i][$type];
	  }
	  if($totRct!=''){
	  	$AVG = $count/$totRct;
	  	return $AVG;
	  }
	  }else{
	  	return "0";
	  } 
	}
