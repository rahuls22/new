function IsAmount(sText) {
	var ValidChars = "0123456789.";
	var IsAmount=true;
	var Char;
	if(sText=="") return false
		for (i = 0; i < sText.length && IsAmount == true; i++) 	
		{ 
			Char = sText.charAt(i); 
			if (ValidChars.indexOf(Char) == -1) 
